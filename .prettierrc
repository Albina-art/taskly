{
  "semi": true,
  "trailingComma": "all",
  "singleQuote": true,
  "printWidth": 100,
  "bracketSameLine": true,
  "arrowParens": "always",
  "tabWidth": 2,
  "importOrder": ["^[next]", "^[react]", "<THIRD_PARTY_MODULES>", "@mui", "@/", "src/", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
